                    }
                ]
            },
            {
                "header": "Socials",
                "items": [
                    {
                        "label": "Discord",
                        "href": "https://discord.gg/pBeEdwXzXm"
                    }
                ]
            }
        ]
    },
    "errors": {
        "404": {
            "redirect": false,
            "title": "That's a swing and a miss!",
            "description": "Seems that wasn't found. Try again?"
        }
    },
    "description": "The UI Kit for Discord that saves boilerplate!",
    "styling": {
        "codeblocks": {
